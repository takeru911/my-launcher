{
  "manifest_version": 3,
  "name": "My Launcher Tab Connector",
  "version": "2.0.0",
  "description": "Enables My Launcher to access and switch to Chrome tabs via WebSocket",
  
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ],
  
  "background": {
    "service_worker": "background.js"
  },
  
  "host_permissions": [
    "ws://localhost:9999"
  ],
  
  "action": {
    "default_title": "My Launcher Tab Connector"
  }
}